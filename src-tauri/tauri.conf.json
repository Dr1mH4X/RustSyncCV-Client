{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "RustSyncCV",
  "version": "0.5.0",
  "identifier": "com.rustsynccv.client",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "fullscreen": false,
        "resizable": true,
        "title": "RustSyncCV",
        "width": 520,
        "height": 420,
        "minWidth": 400,
        "minHeight": 360,
        "transparent": true,
        "decorations": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "category": "Utility",
    "icon": ["../assets/icos/ico.ico"]
  }
}
